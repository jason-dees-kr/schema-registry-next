{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/shared/datatypes/product/$VERSION",
  "clickstreamValidationLevel": "warnings",
	"description": "Array of products",
	"type": "array",
	"items": {
		"type": "object",
		"properties": {
			"name": {
				"description": "Product name",
				"type": "string"
			},
			"sku": {
				"description": "Product SKU",
				"type": "string"
			},
			"product_id": {
				"description": "Full product/retail price",
				"type": "number"
			},
			"numberOfUnits": {
				"description": "Product quntity",
				"type": "number"
			},
			"salePrice": {
				"description": "Sale price of product",
				"type": "number"
			},
			"rating": {
				"description": "Rating of Product",
				"type": "string"
			},
			"sizeOption": {
				"description": "Size of product",
				"type": "string"
			},
			"price": {
				"description": "Full product/retail price",
				"type": "number"
			}
		},
		"required": [
			"name",
			"sku",
			"numberOfUnits",
			"price",
			"rating"
		]
	}
}